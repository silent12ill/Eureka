{
  "name": "Thesis",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --open",
    "server": "nodemon ./backend/server.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "test": "nodemon --exec 'mocha -R min \"backend/test/**/*.js\"'"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      [
        "transform-object-rest-spread"
      ],
      [
        "transform-class-properties"
      ]
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.13.7",
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "connected-react-router": "^4.3.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "mocha": "^4.0.1",
    "mongoose": "^4.13.0",
    "nodemon": "^1.12.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-form": "^2.8.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-slick": "^0.15.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "install": "^0.10.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.0",
    "npm": "^5.5.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.3"
  }
}
